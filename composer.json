{
    "name": "dniccum/nova-documentation",
    "description": "A Laravel Nova tool that allows you to add markdown-based documentation to your administrator's dashboard.",
    "homepage": "https://github.com/dniccum/nova-documentation",
    "keywords": [
        "laravel",
        "nova",
        "markdown",
        "documentation"
    ],
    "authors": [
        {
            "name": "Doug Niccum",
            "email": "dniccumdesign@gmail.com",
            "homepage": "http://dniccumdesign.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "cebe/markdown": "^1.2",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "spatie/yaml-front-matter": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Dniccum\\NovaDocumentation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dniccum\\NovaDocumentation\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
